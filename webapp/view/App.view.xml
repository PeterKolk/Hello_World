<mvc:View controllerName="HelloWorld.HelloWorld.controller.App" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true" xmlns="sap.m">
   <App>
    	<pages>
        	<Page title="{i18n>homePageTitle}">
        		<headerContent>
					<Button
						icon="sap-icon://hello-world"
						press="onOpenDialog"/>
				</headerContent>
            	<content>
            		<mvc:XMLView viewName="HelloWorld.HelloWorld.view.HelloApp"/>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>